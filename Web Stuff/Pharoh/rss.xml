<?xml-stylesheet href="rss_files/global.css" type="text/css"?><html id="feedHandler" xmlns="http://www.w3.org/1999/xhtml" xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <head>
    <title>Viewing Feed</title>
    <link rel="stylesheet" href="rss_files/subscribe.css" type="text/css" media="all"/>
    <script type="application/x-javascript" src="rss_files/subscribe.js"/>
  </head>
  <body onload="SubscribeHandler.writeContent();" onunload="SubscribeHandler.uninit();">
    <div id="feedHeaderContainer">
      <div id="feedHeader" dir="ltr" class="feedBackground">
        <div id="feedIntroText"><xul:description id="feedSubscriptionInfo1"/><xul:description id="feedSubscriptionInfo2"/></div>

<!-- XXXmano this can't have any whitespace in it.  Otherwise you would see
     how much XUL-in-XHTML sucks, see bug 348830 -->
        <div id="feedSubscribeLine"><xul:vbox><xul:hbox align="center"><xul:description id="subscribeUsingDescription" value="Subscribe to this feed using"/><xul:menulist id="handlersMenuList" aria-labelledby="subscribeUsingDescription" sizetopopup="pref" value="" image="chrome://browser/skin/page-livemarks.png" label="Live Bookmarks" description=""><xul:menupopup menugenerated="true" id="handlersMenuPopup"><xul:menuitem id="liveBookmarksMenuItem" label="Live Bookmarks" class="menuitem-iconic" image="chrome://browser/skin/page-livemarks.png" selected="true"/><xul:menuseparator/><xul:menuitem id="selectedAppMenuItem" class="menuitem-iconic" handlerType="client" hidden="true"/><xul:menuitem id="defaultHandlerMenuItem" class="menuitem-iconic" handlerType="client" label="feedreader.exe" image="moz-icon://file:///C:/Program%20Files/FeedReader30/feedreader.exe?size=16"/><xul:menuitem id="chooseApplicationMenuItem" label="Choose Applicationâ€¦"/><xul:menuseparator/><xul:menuitem class="menuitem-iconic" label="Bloglines" handlerType="web" webhandlerurl="http://www.bloglines.com/login?r=/sub/%s" image="data:image/bmp;base64,"/><xul:menuitem class="menuitem-iconic" label="My Yahoo" handlerType="web" webhandlerurl="http://add.my.yahoo.com/rss?url=%s" image="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAASElEQVQ4jWNgGAUM/xkY/uPCJJmDphGG8VqAYQkuCTSDcarDagi6ZlxieMMFm2a8BuCzhSjNRCgkbDO20CY2ev/jwIRtH5oAAKazfoKPSPbRAAAAAElFTkSuQmCC"/><xul:menuitem class="menuitem-iconic" label="Google" handlerType="web" webhandlerurl="http://fusion.google.com/add?feedurl=%s" image="data:image/png;base64,"/></xul:menupopup></xul:menulist></xul:hbox><xul:hbox><xul:checkbox id="alwaysUse" label="Always use Live Bookmarks to subscribe to feeds."/></xul:hbox><xul:hbox align="center"><xul:spacer flex="1"/><xul:button label="Subscribe Now" id="subscribeButton"/></xul:hbox></xul:vbox></div></div>
    </div>

    <script type="application/x-javascript">
      SubscribeHandler.init();
    </script>

    <div id="feedBody">
      <div id="feedTitle">
        <a id="feedTitleLink">
          <img id="feedTitleImage"/>
        </a>
        <div id="feedTitleContainer">
          <h1 id="feedTitleText"/>
          <h2 id="feedSubtitleText"/>
        </div>
      </div>
      <div id="feedContent"/>
    </div>
  </body>
</html>